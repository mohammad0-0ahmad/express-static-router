{
  "name": "express-static-router",
  "version": "0.9.8",
  "description": "Middleware facilitates to implement static router inside your express app.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig-mjs.json && tsc -p tsconfig-cjs.json",
    "dev-cjs": "tsc --watch -p tsconfig-cjs.json",
    "dev-mjs": "tsc --watch -p tsconfig-mjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohammad0-0ahmad/express-static-router.git"
  },
  "keywords": [
    "express",
    "static",
    "router"
  ],
  "author": "Mohammad Ahmad",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "bugs": {
    "url": "https://github.com/mohammad0-0ahmad/express-static-router/issues"
  },
  "homepage": "https://github.com/mohammad0-0ahmad/express-static-router#readme",
  "devDependencies": {
    "@types/node": "^16.4.1",
    "typescript": "^4.3.5"
  },
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    }
  }
}
